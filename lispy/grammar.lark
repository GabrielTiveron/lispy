?start : expr+

?value : SYMBOL 
			 | STRING 
       | LITERAL

?atom	 : value
       | number

number : INT
       | FLOAT

?list  : "[" expr (","expr)* "]"

?expr  : atom
			 | list

// Terminais
SYMBOL.0  : /[-!+\/*@$%^&~<>?|\\\w=]+/
STRING    : /"[^"\\]*(\\[^\n\t\r\f][^"\\]*)*"/
INT.1     : /-?\d+/
FLOAT.2   : /-?\d+\.\d+/
LITERAL   : /#[a-zA-Z]/

%ignore /\s+/
%ignore /;[^\n]*/
