?start : expr

?value : SYMBOL -> symbol
			 | STRING ->string
       | BOOL -> bool

?atom	 : value
       | number
			 | SYMBOL atom atom ->binop

number : INT
       | FLOAT

?term  : atom
       | list

?list  : term term+
       | "(" expr+ ")"->list

?expr  : term
       | "(" expr ")"

// Terminais
SYMBOL.0  : /[-!+\/*@$%^&~<>?|\\\w=]+/
STRING.4    : /"[^"\\]*(\\[^\n\t\r\f][^"\\]*)*"/
INT.1     : /-?\d+/
FLOAT.2   : /-?\d+\.\d+/
BOOL.3	    : /#[tf]/

%ignore /\s+/
%ignore /;[^\n]*/
