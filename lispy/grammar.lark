?start : expr

?value : SYMBOL -> symbol
			 | STRING ->string
       | BOOL -> bool

?atom	 : value
       | number

binop  : SYMBOL atom atom

number : INT
       | FLOAT

?list  : atom
       | "(" list* ")"->list

?expr  : binop
       | list

// Terminais
SYMBOL.0  : /[-!+\/*@$%^&~<>?|\\\w=]+/
STRING.4    : /"[^"\\]*(\\[^\n\t\r\f][^"\\]*)*"/
INT.1     : /-?\d+/
FLOAT.2   : /-?\d+\.\d+/
BOOL.3	    : /#[tf]/

%ignore /\s+/
%ignore /;[^\n]*/
